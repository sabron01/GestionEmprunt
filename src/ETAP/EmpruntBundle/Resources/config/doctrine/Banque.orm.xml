<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ETAP\EmpruntBundle\Entity\Banque" table="BANQUE">
    <indexes>
      <index name="IDX_410E0CBA17A80A88" columns="IDPAYS"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fax" type="bigint" column="FAX" nullable="true"/>
    <field name="mail" type="string" column="MAIL" length="30" nullable="true"/>
    <field name="nombanque" type="string" column="NOMBANQUE" length="20" nullable="true"/>
    <field name="nomresponsable" type="string" column="NOMRESPONSABLE" length="20" nullable="true"/>
    <field name="telephone" type="bigint" column="TELEPHONE" nullable="true"/>
    <many-to-one field="idpays" target-entity="Pays">
      <join-columns>
        <join-column name="IDPAYS" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
