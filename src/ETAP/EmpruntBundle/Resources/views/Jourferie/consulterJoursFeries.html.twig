{% extends "EmpruntBundle::layout.html.twig" %}
{% block title %} {{ parent() }} | Accueil {% endblock %}
{% block centercontent %}
<div class="pageheader">
    <h1 class="pagetitle">Jours Fériés</h1>
    <span class="pagedesc"></span>
</div><!--pageheader-->

<div id="contentwrapper" class="contentwrapper withrightpanel">

    <div id="calendar"></div>

</div><!--contentwrapper-->

<div class="rightpanel">
    <div class="rightpanelinner">
        <div class="widgetbox">
            <div class="title"><h4>événement tunisie</h4></div>
            <div class="widgetcontent">
                <div id="external-events">
                    <div class="external-event ui-draggable" style="position: relative;">Aïd El Kébir</div>
                    <p>Faites glisser les événements dans le calendrier pour ajouter un jour férié.</p>
                </div>  

            </div><!--widgetcontent-->
        </div><!--widgetbox-->
        <div class="widgetbox">
            <div class="title"><h4>événement Angleterre</h4></div>
            <div class="widgetcontent">
                <div id="external-events">
                    <div class="external-event ui-draggable" style="position: relative;">Pâques</div>
                    <p>Faites glisser les événements dans le calendrier pour ajouter un jour férié.</p>
                </div>  

            </div><!--widgetcontent-->
        </div><!--widgetbox-->
    </div><!--rightpanelinner-->
</div><!--rightpanel-->

{% endblock %}
{% block javascripts %}
<script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery-1.5.2.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery-ui-1.8.16.custom.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery.cookie.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/fullcalendar.min.js') }}"></script>

<script type="text/javascript" src="{{ asset('bundles/emprunt/js/custom/general.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/emprunt/js/custom/calendar.js') }}"></script>
<!--[if lt IE 9]>
        <script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
<![endif]-->
{% endblock %}  