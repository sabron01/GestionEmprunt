<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Bienvenue!{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/emprunt/css/style.default.css') }}" type="text/css" />
        <!--[if IE 9]>
            <link rel="stylesheet" media="screen" href="{{ asset('bundles/emprunt/css/style.ie9.css') }}"/>
        <![endif]-->
        <!--[if IE 8]>
            <link rel="stylesheet" media="screen" href="{{ asset('bundles/emprunt/css/style.ie8.css') }}"/>
        <![endif]-->
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="withvernav">
        <div class="bodywrapper">
            <div class="topheader">
                <div class="left">
                    <h1 class="logo"><a href="http://www.etap.com.tn/" target="_blank">ETAP</a></h1> 
                    <h1><span class="slogan">Entreprise Tunisienne d'Activités Pétrolières</span></h1>                      
                    <br clear="all" />

                </div><!--left-->
                {% block headerRight %}{% endblock %}
            </div>
            <div class="header">
            {% block header %}{% endblock %} 
            </div>
            <div class="vernav2 iconmenu">           
            {% block menuVer %}{% endblock %}   
            </div><!--leftmenu-->
            <div class="centercontent">
                {% block centercontent %}{% endblock %}
                <br clear="all" />
            </div><!-- centercontent -->
            {% block javascripts %}
            <script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery-1.7.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery-ui-1.8.16.custom.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery.cookie.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery.uniform.min.js') }}"></script>
       {#     <script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery.flot.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery.flot.resize.min.js') }}"></script>#}
            <script type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/jquery.slimscroll.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/emprunt/js/custom/general.js') }}"></script>
{#            <script type="text/javascript" src="{{ asset('bundles/emprunt/js/custom/dashboard.js') }}"></script>#}
            <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="{{ asset('bundles/emprunt/js/plugins/excanvas.min.js') }}"></script><![endif]-->
            <!--[if lt IE 9]>
                    <script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
            <![endif]-->
            {% endblock %} 
        </div>
    </body>
</html>
